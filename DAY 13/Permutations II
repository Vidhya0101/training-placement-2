def permuteUnique(nums):
    nums.sort()
    res = []
    def backtrack(path, used):
        if len(path) == len(nums):
            res.append(path)
            return
        for i in range(len(nums)):
            if used[i] or (i > 0 and nums[i] == nums[i-1] and not used[i-1]):
                continue
            used[i] = True
            backtrack(path + [nums[i]], used)
            used[i] = False
    backtrack([], [False]*len(nums))
    return res
