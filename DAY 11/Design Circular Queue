class MyCircularQueue:
    def __init__(self, k):
        self.q = [0]*k
        self.size = 0
        self.head = 0

    def enQueue(self, val):
        if self.isFull(): return False
        self.q[(self.head + self.size) % len(self.q)] = val
        self.size += 1
        return True

    def deQueue(self):
        if self.isEmpty(): return False
        self.head = (self.head + 1) % len(self.q)
        self.size -= 1
        return True

    def Front(self):
        return -1 if self.isEmpty() else self.q[self.head]

    def Rear(self):
        return -1 if self.isEmpty() else self.q[(self.head + self.size - 1) % len(self.q)]

    def isEmpty(self):
        return self.size == 0

    def isFull(self):
        return self.size == len(self.q)
